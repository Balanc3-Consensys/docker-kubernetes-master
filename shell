docker rm -f kmaster
docker run \
       -e VPC_ID=$VPC_ID \
       -e ALT_NAMES=$ALT_NAMES \
       --name kmaster \
       --net=host \
       -p 4001:4001 \
       -p 8080:8080 \
       -p 8200:8200 \
       -v `pwd`/etcd-data:/var/lib/etcd-data \
       -v `pwd`/vault-data:/var/lib/vault-data \
       -v /var/log:/var/log \
       -it kubernetes-master bash
